{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/test/**/*.js"
        ]
      }
    ],
    "no-underscore-dangle": "off",
    "no-use-before-define": "off"
  }
}
